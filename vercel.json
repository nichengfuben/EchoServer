{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/vercel_server.py"
    }
  ],
  "functions": {
    "api/vercel_server.py": {
      "runtime": "@vercel/python@3.9"
    }
  }
}